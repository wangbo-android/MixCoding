<h1>Twitter result for <%=search %></h1>
<% if(results.length){ %>

    <ul>
        <% for(var i=0;i<results.length;i++){ %>

            <li>
                <%=results[i].text %> - <em><%=results[i].from_user %>
            </li>
        <% } %>
    </ul>
<% }else{%>

    <p>No results</p>
<% }%>